    \tud@cs@restore{tud@cdgeometry@num}%
    \tud@cs@restore{tud@dim@extrabottommargin}%
    \BeforeSelectAnyPageStyle{\tud@BeforeSelectAnyPageStyle{##1}}%
    \ifcase\tud@cdgeometry@num\relax% typearea
      \tud@loadgeometry{init@typearea}%
