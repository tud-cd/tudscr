          reversemarginpar=false,%
        \fi%
        bindingoffset=\the\ta@bcor,%
        \tud@cdgeometry@hincl,%
        \tud@cdgeometry@fincl,%
      }%
      \edef\tud@res@a{%
        \endgroup%
        \noexpand\tud@cs@use{geometry}{\tud@x@geometry@init,driver=none}%
        \noexpand\@tud@x@geometry@fixmarginstrue%
      }%
    \tud@res@a%
